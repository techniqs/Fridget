<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="at.ac.tuwien.sepm.fridget.client.controllers.EditUserController">
    <VBox prefHeight="500.0" prefWidth="320.0" AnchorPane.bottomAnchor="24.0" AnchorPane.leftAnchor="24.0"
          AnchorPane.rightAnchor="24.0" AnchorPane.topAnchor="24.0">
        <children>
            <Label text="User Information">
                <font>
                    <Font name="System Bold" size="17.0"/>
                </font>
            </Label>
            <Label text="Email Address">
                <VBox.margin>
                    <Insets top="14.0"/>
                </VBox.margin>
            </Label>
            <TextField fx:id="txt_email" disable="true" editable="false"/>
            <Label layoutX="10.0" layoutY="50.0" text="Name">
                <VBox.margin>
                    <Insets top="16.0"/>
                </VBox.margin>
            </Label>
            <TextField fx:id="txt_userName" layoutX="10.0" layoutY="67.0"/>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                <children>
                    <Button mnemonicParsing="false" onAction="#handleEdit" text="Edit"/>
                </children>
            </HBox>
        </children>
    </VBox>
</AnchorPane>
