<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="316.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="at.ac.tuwien.sepm.fridget.client.controllers.RegisterController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Name:" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0"/>
        <TextField fx:id="nameField" layoutX="14.0" layoutY="34.0" onAction="#handleSubmit" prefHeight="25.0"
                   prefWidth="275.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
                   AnchorPane.topAnchor="34.0"/>
        <Label layoutX="14.0" layoutY="74.0" text="Email Address:" AnchorPane.leftAnchor="14.0"
               AnchorPane.topAnchor="74.0"/>
        <TextField fx:id="emailField" layoutX="14.0" layoutY="94.0" onAction="#handleSubmit" prefHeight="25.0"
                   prefWidth="275.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
                   AnchorPane.topAnchor="94.0"/>
        <Label layoutX="14.0" layoutY="134.0" text="Password:" AnchorPane.leftAnchor="14.0"
               AnchorPane.topAnchor="134.0"/>
        <PasswordField fx:id="passwordField" layoutX="14.0" layoutY="154.0" onAction="#handleSubmit" prefHeight="25.0"
                       prefWidth="275.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
                       AnchorPane.topAnchor="154.0"/>
        <Button layoutX="240.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleRegister" text="Register"
                AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <Button layoutX="18.0" layoutY="361.0" mnemonicParsing="false" onAction="#handleBack" text="Back"
                AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0"/>
    </children>
</AnchorPane>
